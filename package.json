{
  "name": "v-img-cropper",
  "version": "1.0.1",
  "description": "Easy to crop and resize your picture before upload",
  "main": "dist/v-img-cropper.ssr.js",
  "browser": "dist/v-img-cropper.esm.js",
  "module": "dist/v-img-cropper.esm.js",
  "unpkg": "dist/v-img-cropper.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "keywords": [
    "vue avatar cropper",
    "avatar",
    "crop picture",
    "cropper",
    "vue image cropper",
    "image crop",
    "avatar-cropper",
    "image cropper",
    "img resize",
    "vuejs",
    "javascript",
    "v-img-cropper"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hieutran094/vue-img-cropper.git"
  },
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "bootstrap": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hieutran094/vue-img-cropper/issues"
  },
  "homepage": "https://github.com/hieutran094/vue-img-cropper#readme"
}
